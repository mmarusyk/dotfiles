{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.wordWrap": "on",
    "editor.tabSize": 2,
    "editor.fontSize": 14,
    "editor.lineHeight": 18,
    "window.titleBarStyle": "custom",
    "files.insertFinalNewline": true,
    "ruby.intellisense": "rubyLocate",
    "workbench.editor.enablePreview": true,
    "gitlens.statusBar.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.views.commits.showBranchComparison": false,
    "gitlens.views.commits.pullRequests.enabled": false,
    "gitlens.views.commits.files.compact": false,
    "gitlens.views.commits.avatars": false,
    "gitlens.views.branches.showBranchComparison": false,
    "gitlens.views.branches.pullRequests.enabled": false,
    "gitlens.views.branches.files.compact": false,
    "gitlens.views.branches.avatars": false,
    "gitlens.views.remotes.pullRequests.enabled": false,
    "gitlens.views.remotes.files.compact": false,
    "gitlens.views.remotes.avatars": false,
    "gitlens.views.stashes.files.compact": false,
    "gitlens.views.tags.files.compact": false,
    "gitlens.views.tags.avatars": false,
    "gitlens.views.contributors.pullRequests.enabled": false,
    "gitlens.views.contributors.files.compact": false,
    "gitlens.views.contributors.avatars": false,
    "gitlens.views.searchAndCompare.pullRequests.enabled": false,
    "gitlens.views.searchAndCompare.files.compact": false,
    "gitlens.views.searchAndCompare.avatars": false,
    "gitlens.changes.locations": [
        "overview",
        "gutter"
    ],
    "gitlens.heatmap.locations": [
        "overview",
        "gutter"
    ],
    "workbench.editorAssociations": [
        {
            "viewType": "default",
            "filenamePattern": "git-rebase-todo"
        }
    ],
    "gitlens.menus": {
        "editor": false,
        "editorGroup": {
            "blame": true,
            "compare": true
        },
        "editorTab": false,
        "explorer": false,
        "scm": false,
        "scmGroupInline": false,
        "scmGroup": false,
        "scmItem": false
    },
    "gitlens.currentLine.scrollable": false,
    "editor.rulers": [
        80
    ],
    "files.associations": {
        ".vimrc": "shellscript"
    },
    "json.maxItemsComputed": 10000,
    "files.autoSave": "afterDelay",
    "gitlens.codeLens.enabled": false,
    "files.exclude": {
        ".tags": true,
        "tags": true
    }
}
